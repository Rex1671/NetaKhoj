# Server
NODE_ENV=production
PORT=3000
HOST=0.0.0.0

# Security
API_SECRET=your_secret_key_here
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Cache
REDIS_URL=redis://localhost:6379
CACHE_TTL_PRS=3600
CACHE_TTL_CANDIDATE=1800

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Scraper
SCRAPER_TIMEOUT=30000
SCRAPER_RETRIES=3
MAX_BROWSERS=3